@model ShopCartViewModel
<div class="container">
    <h1 class="text-center text-white">Ваша корзина:</h1>
    @foreach (var el in Model.shopCart.listShopItems)
    {
    <div class="alert alert-warning mt-3">
        <b>Автомобиль: </b> @el.car.name<br />
        <b>Цена:</b> @el.car.price.ToString("c")
    </div>
    }
    <hr />
    <p><a class="btn btn-danger" asp-controller="Order" asp-action="Checkout">Оплатить</a></p>
</div>